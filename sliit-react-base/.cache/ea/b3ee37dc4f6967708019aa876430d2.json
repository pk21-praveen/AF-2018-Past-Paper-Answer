n\r\n    render() {\r\n        return (\r\n            <div>\r\n                <h4>Subject List</h4>\r\n                <br/>\r\n                <table className=\"table table-striped table-bordered\">\r\n                    <thead>\r\n                    <tr>\r\n                        <th>Name</th>\r\n                        <th>Description</th>\r\n                        <th>Amount</th>\r\n                        <th>Delete</th>\r\n                    </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                    {this.state.subjectsList.map((subject, i) => {\r\n                        return (\r\n                            <tr key={i}>\r\n                                <td>{subject.name}</td>\r\n                                <td>{subject.description}</td>\r\n                                <td>{subject.amount}</td>\r\n                                <td><input type=\"button\" value=\"Delete\" className=\"btn btn-danger\"\r\n                                           onClick={() => this.onClickDeleteSubject(subject._id)}/></td>\r\n                            </tr>\r\n                        )\r\n                    })\r\n                    }\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n        );\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"e11990840c613855f66f615ae99c072e","cacheData":{"env":{}}}